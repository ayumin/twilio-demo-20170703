{
  "name": "Twilio Sample on Heroku",
  "description": "Sending SMS message sample powered by Twilio",
  "repository": "https://github.com/ayumin/twilio-demo-20170703",
  "keywords": ["ruby", "twilio"],
  "scripts": {
  },
  "env": {
    "HEROKU_APP_NAME": {
      "required": true
    },
    "LANG": {
      "required": true,
      "value": "en_US.UTF-8"
    },
    "RACK_ENV": {
      "required": true,
      "value": "production"
    },
    "TWILIO_ACCOUNT_SID": {
      "required": true
    },
    "TWILIO_AUTH_TOKEN": {
      "required": true
    },
    "TWILIO_PHONE": {
      "required": true
    }
  },
  "formation": {
  },
  "addons": [

  ],
  "buildpacks": [
    {
      "url": "heroku/ruby"
    }
  ]
}
